<%= form_with(model: @listing, local: true, data: {remote: true}) do |form| %>
<%= form.text_area :listing_code, :size => "80x10", :class => "listing-code", :required => true%>
<div class="right-form">
  <div class="save-delete">
    <%= button_to '上書き保存', @listing, {method: :save, class: "btn btn-primary"} %><br><br>
    <%= button_to '削除する', @listing, method: :delete,class: "btn btn-danger", data: { confirm: '本当に削除しますか?' } %>
  </div>
</div>
<% end  %>
